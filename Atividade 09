using System;

class Program
{
    static void Main()
    {

        Console.Write("Qual seu nome?: " );
        string nome = (Console.ReadLine());
          bool sim = Convert.ToBoolean(nome.Equals("Wilson", StringComparison.OrdinalIgnoreCase) ||
                                       nome.Equals("Gloria", StringComparison.OrdinalIgnoreCase));
        if (sim == true){
             Console.WriteLine("Olá, bem-vindo(a) de volta!");
           }
        else{
              Console.WriteLine("Nome não consta no sistema.");
           }

    }
}
